  "scripts": {
    "awslocal": "aws --endpoint http://localhost:4566",
    "compile": "run-p compile:contact-retrieve-recording-url compile:contact-retrieve-transcript",
    "compile:hrm-ssm-cache": "cd src/lib/hrm-ssm-cache && npm ci && cd ../../../ && npx tsc src/lib/hrm-ssm-cache/*.ts",
    "compile:contact-retrieve-recording-url": "cd src/contact-retrieve-recording-url && npm ci && cd ../../ && npx tsc src/contact-retrieve-recording-url/*.ts",
    "compile:contact-retrieve-transcript": "cd src/contact-retrieve-transcript && npm ci && cd ../../ && npx tsc src/contact-retrieve-transcript/*.ts",
    "docker:build": "run-p docker:build:contact-retrieve-recording-url docker:build:contact-retrieve-transcript",
    "docker:build:contact-retrieve-recording-url": "docker build -t lambdas-contact-retrieve-recording-url --build-arg lambda_name=contact-retrieve-recording-url -f Dockerfile ../",
    "docker:build:contact-retrieve-transcript": "docker build -t lambdas-contact-retrieve-transcript --build-arg lambda_name=contact-retrieve-transcript -f Dockerfile ../",
    "docker:build-and-run:contact-retrieve-recording-url": "run-s docker:build:contact-retrieve-recording-url docker:compose:up",
    "docker:build-and-run:contact-retrieve-transcript": "run-s docker:build:contact-retrieve-transcript docker:compose:up",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "dev": "run-s docker:compose:up local:bootstrap local:deploy docker:compose:logs",
    "local": "cdklocal",
    "local:bootstrap": "cdklocal bootstrap",
    "local:deploy": "cdklocal deploy --all --outputs-file ./cdk/outputs.json",
    "local:destroy": "cdklocal destroy --all",
    "local:diff": "cdklocal diff",
    "local:synth": "cdklocal synth --all",
    "test:docker:contact-retrieve-recording-url": "curl -XPOST \"http://localhost:9001/2015-03-31/functions/function/invocations\" -d '{}'",
    "test:docker:contact-retrieve-transcript": "curl -XPOST \"http://localhost:9000/2015-03-31/functions/function/invocations\" -d \"$(cat src/contact-retrieve-transcript/testPayload.json)\"",
    "test:localstack:contact-retrieve-transcript": "ts-node tests/sendMockContactMessage.ts contact-retrieve-transcript"
  }
}
